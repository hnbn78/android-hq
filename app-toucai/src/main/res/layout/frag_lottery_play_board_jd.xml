<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    android:clipToPadding="false">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="15dp"
        android:clipChildren="false"
        android:clipToPadding="false">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:orientation="vertical">

            <com.skyline.widget.layout.RoundCornerLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/sh_lottery_play_jd_head_view"
                android:elevation="2dp"
                android:outlineProvider="background"
                app:bottomLeftEnabled="false"
                app:bottomRightEnabled="false"
                app:cornerRadius="5dip"
                app:topLeftEnabled="true"
                app:topRightEnabled="true">

                <com.desheng.app.toucai.view.PlayHeadView
                    android:id="@+id/vHead"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

            </com.skyline.widget.layout.RoundCornerLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="2">

                <android.support.v7.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    app:cardCornerRadius="4dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="1">

                        <com.desheng.app.toucai.view.PlayBoardJDView
                            android:id="@+id/vgDigit0"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />

                        <View
                            android:id="@+id/v_divider"
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:background="#d8d8da"
                            android:visibility="gone" />

                        <com.desheng.app.toucai.view.PlayBoardJDView
                            android:id="@+id/vgDigit1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />
                    </LinearLayout>
                </android.support.v7.widget.CardView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="2">

                <android.support.v7.widget.CardView
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    app:cardCornerRadius="4dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="2">

                        <com.desheng.app.toucai.view.PlayBoardJDView
                            android:id="@+id/vgDigit2"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />

                        <View
                            android:id="@id/v_divider"
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:background="#d8d8da"
                            android:visibility="gone" />

                        <com.desheng.app.toucai.view.PlayBoardJDView
                            android:id="@+id/vgDigit3"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />
                    </LinearLayout>
                </android.support.v7.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="2">

                <android.support.v7.widget.CardView
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    app:cardCornerRadius="4dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="2">

                        <com.desheng.app.toucai.view.PlayBoardJDView
                            android:id="@+id/vgDigit4"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />

                        <View
                            android:id="@id/v_divider"
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:background="#d8d8da"
                            android:visibility="gone" />

                        <com.desheng.app.toucai.view.PlayBoardJDView
                            android:id="@+id/vgDigit5"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />

                    </LinearLayout>
                </android.support.v7.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="2">

                <android.support.v7.widget.CardView
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    app:cardCornerRadius="4dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="2">

                        <com.desheng.app.toucai.view.PlayBoardJDView
                            android:id="@+id/vgDigit6"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />

                        <View
                            android:id="@id/v_divider"
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:background="#d8d8da"
                            android:visibility="gone" />

                        <com.desheng.app.toucai.view.PlayBoardJDView
                            android:id="@+id/vgDigit7"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />

                    </LinearLayout>
                </android.support.v7.widget.CardView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="2">

                <android.support.v7.widget.CardView
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    app:cardCornerRadius="4dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="2">

                        <com.desheng.app.toucai.view.PlayBoardJDView
                            android:id="@+id/vgDigit8"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />

                        <View
                            android:id="@id/v_divider"
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:background="#d8d8da"
                            android:visibility="gone" />

                        <com.desheng.app.toucai.view.PlayBoardJDView
                            android:id="@+id/vgDigit9"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />
                    </LinearLayout>
                </android.support.v7.widget.CardView>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>