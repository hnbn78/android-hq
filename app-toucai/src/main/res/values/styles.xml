    <style name="btn_mission" parent="@style/Widget.AppCompat.Button.Borderless">
        <item name="android:layout_width">90dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textColor">#CC2B29</item>
        <item name="android:textSize">@dimen/text_normal</item>
        <item name="android:background">@drawable/sh_btn_mission_round</item>
    </style>

    <style name="iv_mission_delete">
        <item name="android:layout_width">22dp</item>
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:src">@mipmap/btn_new_mission_delete</item>
    </style>
    <!--lottery play 样式 end-->

    <style name="progressBarHorizontal" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/update_progress_style</item>
        <item name="android:minHeight">10dp</item>
        <item name="android:maxHeight">10dp</item>
    </style>

    <style name="indcatorTextStyle">
        <item name="android:textSize">15dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="indcatorTextStyle2">
        <item name="android:textSize">15dp</item>
    </style>

    <style name="home_tab_style">
        <item name="android:textSize">14dp</item>
    </style>

    <style name="text_subhead_black">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_title</item>
        <item name="android:textSize">@dimen/text_subhead</item>
        <item name="android:textIsSelectable">true</item>
    </style>

    <style name="text_body">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_sub</item>
        <item name="android:textSize">@dimen/text_body</item>
    </style>

</resources>