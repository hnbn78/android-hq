<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="@color/colorPrimaryInverse"
    android:id="@+id/toup_fragment">





    <RelativeLayout
        android:id="@+id/first__layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:layout_marginTop="10dp"
        android:padding="2dp"
        >


        <TextView
            android:id="@+id/modify_funds"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="@string/bank_scan"
            android:textSize="16sp"

            />

        <ImageView
            android:id="@+id/modify_founds_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:scaleType="centerCrop"
            android:src="@mipmap/ic_common_arrow_right"
            android:layout_centerVertical="true"

            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/myaccount_title"
        android:layout_below="@+id/first__layout"
         android:id="@+id/line_first"
        />
    <RelativeLayout
        android:id="@+id/second__layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line_first"
        android:layout_margin="3dp"
        android:layout_marginTop="10dp"
        android:padding="2dp"
        >


        <TextView
            android:id="@+id/pay_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="@string/bank_net"
            android:textSize="16sp"

            />

        <ImageView
            android:id="@+id/pay_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:scaleType="centerCrop"
            android:src="@mipmap/ic_common_arrow_right"
            android:layout_centerVertical="true"

            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/myaccount_title"
        android:layout_below="@+id/second__layout"
         android:id="@+id/line_second"
        />

    <RelativeLayout
        android:id="@+id/third__layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line_second"
        android:layout_margin="3dp"
        android:layout_marginTop="10dp"
        android:padding="2dp"
        >


        <TextView
            android:id="@+id/net_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="@string/bank_scan_qq"
            android:textSize="16sp"

            />

        <ImageView
            android:id="@+id/net_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:scaleType="centerCrop"
            android:src="@mipmap/ic_common_arrow_right"
            android:layout_centerVertical="true"

            />



    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/myaccount_title"
        android:layout_below="@+id/third__layout"
        android:id="@+id/line_third"
        />
<RelativeLayout
    android:id="@+id/four__layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_below="@+id/third__layout"
    android:layout_margin="3dp"
    android:layout_marginTop="10dp"
    android:padding="2dp"
    >


    <TextView
        android:id="@+id/four_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:text="@string/bank_scan_qq_web"
        android:textSize="16sp"

        />

    <ImageView
        android:id="@+id/four_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:scaleType="centerCrop"
        android:src="@mipmap/ic_common_arrow_right"
        android:layout_centerVertical="true"

        />
</RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/myaccount_title"
        android:layout_below="@+id/four__layout"
        android:id="@+id/four_third"
        />
    <RelativeLayout
        android:id="@+id/five__layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/four__layout"
        android:layout_margin="3dp"
        android:layout_marginTop="10dp"
        android:padding="2dp"
        >


        <TextView
            android:id="@+id/five_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="@string/bank_pay_qq"
            android:textSize="16sp"

            />

        <ImageView
            android:id="@+id/five_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:scaleType="centerCrop"
            android:src="@mipmap/ic_common_arrow_right"
            android:layout_centerVertical="true"

            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/myaccount_title"
        android:layout_below="@+id/four__layout"
        android:id="@+id/five_third"
        />


</RelativeLayout>
